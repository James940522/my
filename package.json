{
  "name": "apr",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "update-deps": "pnpm update -r --latest",
    "sync-workspace": "pnpm install"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "workspace:*"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.5.4",
      "next": "^15.3.0",
      "react": "^19.0.0",
      "react-dom": "^19.0.0"
    },
    "catalogType": [
      "react19",
      "react-query5"
    ]
  }
}
